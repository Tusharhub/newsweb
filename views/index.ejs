        // Function to update the content of the specified element with the current date and day
        function updateDateAndDay() {
            // Specify the time zone (e.g., 'America/New_York')
            const timeZone = 'Asia/Kolkata';

            // Get the current date and time in the specified time zone
            const currentDate = new Date().toLocaleString('en-US', { timeZone: timeZone });

            // Extract the date and day
            const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };
            const formattedDate = new Date(currentDate).toLocaleDateString('en-US', options);

            // Update the content of the specified element with the formatted date and day
            const dateAndDayElement = document.getElementById('dateLink');
            dateAndDayElement.textContent = formattedDate;
        }

        // Call the function when the page loads
        updateDateAndDay();
    </script>
